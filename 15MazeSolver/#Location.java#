public class Location implements Comparable<Location>{
    private int row, col;
    private int distToGoal;
    private int distToStart;
    private Location previous;
    private boolean aStar;

    public Location(int r, int c, Location _previous, int _distToStart, int _distToGoal, boolean _aStar) {
	row = r;
	col = c;
	previous = _previous;
	distToStart = _distToStart;
	distToGoal = _distToGoal;
	aStar = _aStar;
    }
    
    public int getRow() {
	return row;
    }
    public int getCol() {
	return col;
    }
    public int getDistToGoal() {
	return distToGoal;
    }
    public int getDistToStart() {
	return distToStart;
    }
    public Location getPrevious() {
	return previous;
    }
    public boolean isAStar () {
	return aStar;
    }
    
