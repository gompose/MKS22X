public class MyLinkedList {
    private LNode start;
    private int size;



    private class LNode {
	public int value;
	public LNode next;
    
	public LNode (int _value, LNode nextNode) {
	    value = _value;
	    next = nextNode;
	}
	public LNode (int _value) {
	    value = _value;
	    next = null;
	}
	public LNode (LNode nextNode) {
	    value = 0;
	    next = nextNode;
	}
	public LNode () {
	    value = 0;
	    next = null;
	}
    }
}
